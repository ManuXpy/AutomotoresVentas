	<queryString>
		<![CDATA[SELECT
factura.id_factura,
factura.fecha_factura,
factura.clientes_cod_cliente,
factura.empleados_emple_codigo,
factura_detalle.productos_produ_codigo,
factura_detalle.factura_id_factura,
factura_detalle.cantidad,
productos.produ_precio_vent,
impuesto.descri_impuesto,
estado.estado_descripcion,
CONCAT(productos.produ_descripcion,"  ",productos.productos_marca),
CONCAT(empleados.emple_nombres,"  ",empleados.emple_apellido),
CONCAT(clientes.nombre_cliente,"  ",clientes.razonsocial_cliente)
FROM
factura_detalle,
factura ,
productos,
empleados,
clientes,
impuesto,
estado
WHERE
productos.produ_codigo=factura_detalle.productos_produ_codigo AND
empleados.emple_codigo=factura.empleados_emple_codigo AND
clientes.cod_cliente=factura.clientes_cod_cliente AND
productos.impuesto_id_impuesto=impuesto.id_impuesto  AND
productos.estado_id_estado=estado.id_estado AND
factura.id_factura=factura_detalle.factura_id_factura AND
factura_id_factura= $P{facturaCodigo}]]>
	</queryString>
